worker_a c {
    (sleep 100)
    (send c 0)
}

worker_b c {
    (sleep 50)
    (send c 1)
}

f c {
    let x {
        (receive c)
    }
    if (= x 0) {
        (printf " . received `a`\n")
    } else if (= x 1) {
        (printf " . received `b`\n")
    } else {
        (panic)
    }
}

main {
    let c {
        (channel)
    }

    (spawn worker_a c)
    (spawn worker_b c)

    (f c)
    (f c)

    (printf "Done!\n")
}
